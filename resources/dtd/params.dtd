<!ELEMENT params 	(attribute|node+|param)>
<!ATTLIST params
	all		CDATA	#IMPLIED
	inbase		CDATA	#IMPLIED>
<!ELEMENT attribute	(#PCDATA)>
<!ATTLIST attribute
	name		CDATA #REQUIRED>
<!ELEMENT node		(node+|param+)>
<!ATTLIST node
	name	CDATA	#REQUIRED>
<!ELEMENT param		EMPTY>
<!ATTLIST param
	name		CDATA	#REQUIRED
	full_name	CDATA	#REQUIRED
	short_name	CDATA	#REQUIRED
	value		CDATA	#REQUIRED
	unit		CDATA	#REQUIRED
	ipc_ind		CDATA	#REQUIRED
	prec		CDATA	#REQUIRED>
	
	
