	<package name="egcs"></package>

	<szarp>1.746 2004/08/30 08:12:51</szarp>
</sysinfo>

-->

<grammar xmlns="http://relaxng.org/ns/structure/1.0">

	<!--
	  G³ównym elementem jest element 'sysinfo'. 
	  Atrybuty:
	  	"version" - wersja, na razie powinno byæ 1.0
	-->
	
	<define name="sysinfo">
		<element name="sysinfo" 
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<ref name="attlist.sysinfo"/>
			<interleave>
				<ref name="hostname"/>
				<ref name="uname"/>
				<ref name="distro"/>
				<zeroOrMore>
					<ref name="program"/>
				</zeroOrMore>
				<zeroOrMore>
					<ref name="package"/>
				</zeroOrMore>
				<zeroOrMore>
					<ref name="library"/>
				</zeroOrMore>
				<ref name="szarp"/>
				<ref name="szarp-updater"/>
			</interleave>
		</element>
	</define>
	
	<define name="attlist.sysinfo">
		<attribute name="version"/>
	</define>

	<define name="hostname">
		<element name="hostname"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<text/>
		</element>
	</define>

	<define name="uname">
		<element name="uname"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<text/>
		</element>
	</define>

	<define name="distro">
		<element name="distro"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<text/>
		</element>
	</define>


	<define name="program">
		<element name="program"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<ref name="attlist.name"/>
			<text/>
		</element>
	</define>

	<define name="package">
		<element name="package"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<ref name="attlist.name"/>
			<text/>
		</element>
	</define>

	<define name="library">
		<element name="library"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<ref name="attlist.name"/>
			<text/>
		</element>
	</define>

	<define name="szarp">
		<element name="szarp"
				ns="http://www.praterm.com.pl/SZARP/sysinfo">
			<text/>
		</element>
	</define>

	<define name="szarp-updater">
		<element name="szarp-updater" ns="http://www.praterm.com.pl/SZARP/sysinfo">
		    <text/>
		</element>
	</define>

	<define name="attlist.name">
		<attribute name="name"/>
	</define>

	
	<!-- G³ówny tag dokumentu -->

	<start>
		<choice>
			<ref name="sysinfo"/>
		</choice>
	</start>
  
</grammar>

