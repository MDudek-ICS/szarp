		</optional>
		<optional>
			<attribute name="ip" ns="http://www.praterm.com.pl/SZARP/ipk-extra"/>
		</optional>
		<optional>
			<attribute name="tag" ns="http://www.praterm.com.pl/SZARP/ipk-extra"/>
		</optional>
		<optional>
			<attribute name="id" ns="http://www.praterm.com.pl/SZARP/ipk-extra"/>
		</optional>
		<optional>
			<attribute name="param" ns="http://www.praterm.com.pl/SZARP/ipk-extra"/>
		</optional>
		<optional>
			<attribute name="transform" ns="http://www.praterm.com.pl/SZARP/ipk-extra">
				<choice>
					<value>lsw</value>
					<value>msw</value>
				</choice>
			</attribute>
		</optional>
		<optional>
			<attribute name="prec" ns="http://www.praterm.com.pl/SZARP/ipk-extra">
				<data type="integer"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="devid" ns="http://www.praterm.com.pl/SZARP/ipk-extra"/>
		</optional>
		<optional>
			<attribute name="special" ns="http://www.praterm.com.pl/SZARP/ipk-extra">
				<value>prev</value>
			</attribute>
		</optional>
		<optional>
			<attribute name="val_op2" ns="http://www.praterm.com.pl/SZARP/ipk-extra">
				<choice>
					<value>MSD</value>
					<value>LSD</value>
