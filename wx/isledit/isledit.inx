<inkscape-extension>
    <_name>ISL Editor</_name>
    <id>pl.com.praterm.isledit</id>
    <dependency type="executable" location="absolute">/opt/szarp/bin/isledit</dependency>
	<param name="file" _gui-text="Path to SZARP configuration" type="string">/opt/szarp/.../config/params.xml</param>
    <effect>
        <object-type>all</object-type>
    </effect>
    <script>
        <command reldir="absolute">/opt/szarp/bin/isledit</command>
    </script>
</inkscape-extension>
